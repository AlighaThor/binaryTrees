{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": ["eslint:recommended"],
    "rules": {
        "quotes": ["error", "single", { "avoidEscape": true }],
        "eqeqeq": "error",
        "no-console": "warn",
        "no-var": "error",
        "strict": "error",
        "camelcase": "error"
    }
}
